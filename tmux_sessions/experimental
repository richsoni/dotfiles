tmux has-session -t experimenal
if [ $? != 0 ]
then
  tmux new-session -s experimental -n experimental -d
  #create splits, and windows
      # first 3
        tmux send-keys -t experimental 'cd ~/workspace/exchange/' C-m
        tmux split-window -p 85 -h -t experimental
        tmux select-pane -t experimental:1.1
        tmux split-window -p 85 -v -t experimental
        tmux select-pane -t experimental:1.3
        tmux split-window -p 15 -h -t experimental
