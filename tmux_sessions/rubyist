      tmux send-keys -t rubyist:1.1 'vi' C-m
      tmux split-window -v -t rubyist
      tmux send-keys -t rubyist:1.3 'cd ~/Code/ruby/rubyist_examples' C-m
      tmux send-keys -t rubyist:1.3 'irb' C-m
