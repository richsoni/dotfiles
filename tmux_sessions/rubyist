tmux has-session -t rubyist
if [ $? != 0 ]
then
  tmux new-session -s rubyist -n rubyist -d
  #create splits, and windows
      tmux send-keys -t rubyist 'cd ~/Code/ruby/rubyist_examples' C-m
      tmux split-window -h -t rubyist
