#PATH STUFF
  export CONFIG_PATH=~/richsoni-config
  export NODE_PATH='/usr/local/lib/node_modules'
  #Folder
  export PATH=./bin:$PATH
  export PATH=./node_modules/.bin:$PATH
  #User
  export PATH=$PATH:/usr/local/bin
  export PATH=$PATH:$HOME/.bin
  export PATH=$PATH:$HOME/bin
  export PATH=$PATH:$HOME/.rvm/bin # Add RVM to PATH for scripting
  #System
  export PATH=$PATH:$NODE_PATH
  export PATH=$PATH:/usr/local/heroku/bin
  export PATH=$PATH:/usr/local/sbin
  export PATH=$PATH:/usr/local/share
  export PATH=$PATH:/Applications/Postgres93.app/Contents/MacOS/bin

# Less Colors for Man Pages
  export LESS_TERMCAP_mb=$'\E[01;31m'       # begin blinking
  export LESS_TERMCAP_md=$'\E[01;38;5;74m'  # begin bold
  export LESS_TERMCAP_me=$'\E[0m'           # end mode
  export LESS_TERMCAP_se=$'\E[0m'           # end standout-mode
  export LESS_TERMCAP_so=$'\E[38;5;246m'    # begin standout-mode - info box
  export LESS_TERMCAP_ue=$'\E[0m'           # end underline
  export LESS_TERMCAP_us=$'\E[04;38;5;146m' # begin underline
  export LESS_TERMCAP_mb=$(tput bold; tput setaf 2) # green
  export LESS_TERMCAP_md=$(tput bold; tput setaf 6) # cyan
  export LESS_TERMCAP_me=$(tput sgr0)
  export LESS_TERMCAP_so=$(tput bold; tput setaf 3; tput setab 4) # yellow on blue
  export LESS_TERMCAP_se=$(tput rmso; tput sgr0)
  export LESS_TERMCAP_us=$(tput smul; tput bold; tput setaf 7) # white
  export LESS_TERMCAP_ue=$(tput rmul; tput sgr0)
  export LESS_TERMCAP_mr=$(tput rev)
  export LESS_TERMCAP_mh=$(tput dim)
  export LESS_TERMCAP_ZN=$(tput ssubm)
  export LESS_TERMCAP_ZV=$(tput rsubm)
  export LESS_TERMCAP_ZO=$(tput ssupm)
  export LESS_TERMCAP_ZW=$(tput rsupm)
  export LC_CTYPE=en_US.UTF-8
  export LC_ALL=en_US.UTF-8
# define preferred tools
  export EDITOR=vim
  export PAGER=less
#global options
  export HISTIGNORE='ls:rl:source:pwd:clear:history'
  export HISTCONTROL=ignoredups
  # export NAG_IGNORE=true
#setup rvm
  [[ -s "$HOME/.rvm/scripts/rvm" ]] && source "$HOME/.rvm/scripts/rvm"
#lynx
  export WWW_HOME='http://duckduckgo.com'
cat $CONFIG_PATH/ascii/heis
nag
